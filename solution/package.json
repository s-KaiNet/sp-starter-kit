{
  "name": "sp-starter-kit",
  "version": "1.5.2",
  "private": true,
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "tsc:watch": "tsc -p tsconfig.json -w",
    "tsc": "tsc -p tsconfig.json",
    "serve": "cross-env NODE_OPTIONS=--max_old_space_size=8192 gulp serve",
    "build:dev": "gulp clean && npm run tsc && cross-env NODE_OPTIONS=--max_old_space_size=8192 gulp bundle && gulp package-solution",
    "build:prod": "gulp clean && cross-env NODE_OPTIONS=--max_old_space_size=8192 gulp bundle --ship && gulp package-solution --ship",
    "bundle:dev": "npm run tsc && cross-env NODE_OPTIONS=--max_old_space_size=8192 gulp bundle",
    "bundle:prod": "cross-env NODE_OPTIONS=--max_old_space_size=8192 gulp bundle --ship",
    "clean": "gulp clean",
    "test": "gulp test"
  },
  "dependencies": {
    "@microsoft/decorators": "1.9.1",
    "@microsoft/sp-application-base": "1.9.1",
    "@microsoft/sp-core-library": "1.9.1",
    "@microsoft/sp-dialog": "1.9.1",
    "@microsoft/sp-listview-extensibility": "1.9.1",
    "@microsoft/sp-lodash-subset": "1.9.1",
    "@microsoft/sp-office-ui-fabric-core": "1.9.1",
    "@microsoft/sp-webpart-base": "1.9.1",
    "@microsoft/teams-js": "^1.4.1",
    "@pnp/common": "^1.0.4-4",
    "@pnp/logging": "^1.0.4-4",
    "@pnp/odata": "^1.0.4-4",
    "@pnp/sp": "^1.0.4-4",
    "@pnp/spfx-controls-react": "1.11.0",
    "@pnp/spfx-property-controls": "1.13.1",
    "@types/es6-promise": "0.0.33",
    "@types/react": "16.7.22",
    "@types/react-dom": "16.8.0",
    "@types/webpack-env": "1.13.1",
    "react": "16.3.2",
    "react-dom": "16.3.2",
    "@uifabric/utilities": "5.33.0"
  },
  "devDependencies": {
    "@microsoft/microsoft-graph-types": "^1.8.0",
    "@microsoft/rush-stack-compiler-3.3": "^0.2.36",
    "@microsoft/sp-build-web": "1.9.1",
    "@microsoft/sp-module-interfaces": "1.9.1",
    "@microsoft/sp-tslint-rules": "1.9.1",
    "@microsoft/sp-webpart-workbench": "1.9.1",
    "@types/chai": "3.4.34",
    "@types/mocha": "2.2.38",
    "@types/yargs": "^13.0.2",
    "ajv": "~5.2.2",
    "colors": "^1.3.3",
    "cross-env": "^5.2.0",
    "gulp": "~3.9.1",
    "hard-source-webpack-plugin": "^0.13.1",
    "speed-measure-webpack-plugin": "^1.3.1",
    "tslint-microsoft-contrib": "5.0.0",
    "yargs": "^14.0.0"
  }
}
